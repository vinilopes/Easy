                    <table class="table table-collapse-phone">
                        <tr>
                            <td>
                                <b>#</b>
                            </td>
                            <td>
                                <b>Nome</b>
                            </td>
                            <td>
                                <b>Email</b>
                            </td>
                            <td>
                                <b>Skype</b>
                            </td>
                            <td>
                                <b>Telefone</b>
                            </td>
                            <td>
                                <b>Ações</b>
                            </td>

                        </tr>
                        <tr ng-repeat="Cand in candidatos">
                            <td>{{Cand.codigo}}</td>
                            <td>{{Cand.nome}}</td>
                            <td>{{Cand.email}}</td>
                            <td>{{Cand.skype}}</td>
                            <td>{{Cand.telefone}}</td>
                            <td>
                                <button type="submit" class="btn btn-primary"  data-target="#ModalTalent" data-toggle="modal" ng-click="ConsultCandidate(Cand)">
                                    <span class="glyphicon glyphicon-search"></span>
                                </button>
                                <button type="submit" class="btn btn-success" data-target="#ModalTalent" data-toggle="modal" ng-click="UpdateCandidate(Cand)">
                                    <span class="glyphicon glyphicon-pencil"></span>
                                </button>
                                <button type="submit" class="btn btn-danger"   ng-click="DeleteCandidate(Cand)">
                                    <span class="glyphicon glyphicon-remove"></span>
                                </button>
                            </td>
                        </tr>
                    </table>
